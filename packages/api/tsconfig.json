{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"strict": true,
		"lib": ["esnext"],
		"types": ["@cloudflare/workers-types", "bun-types"],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx"
	},
	"include": ["index.ts", "drizzle.config.ts", "./src/**/*.ts", "./src**/*.tsx"]
}
